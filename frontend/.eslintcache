d.qty : 1)\r\n  const history = useHistory()\r\n\r\n  const dispatch = useDispatch()\r\n  const { product, error, loading } = useSelector((state) => state.product)\r\n  const { cartItems } = useSelector((state) => state.cart)\r\n  useEffect(() => {\r\n    const fetchProduct = async () => {\r\n      console.log(product._id, id)\r\n      if (\r\n        Object.keys(product).length === 0 ||\r\n        location.search.split(\"=\")[1] === \"cart\"\r\n      )\r\n        dispatch(productDetailAction(id))\r\n    }\r\n    fetchProduct()\r\n  }, [location.pathname])\r\n\r\n  const buyNowHandler = () => {\r\n    history.push(\"/shipping\")\r\n  }\r\n  const addToCart = () => {\r\n    const match = cartItems ? cartItems.find((each) => each._id === id) : false\r\n\r\n    if (!match) {\r\n      dispatch(cartAction(id, qty))\r\n      setCartCount(Number(cartCount) + Number(qty))\r\n    } else if (match) {\r\n      dispatch(removeAction(match))\r\n      setCartCount(Number(cartCount) - Number(qty))\r\n    }\r\n  }\r\n  return (\r\n    <StyledDetail>\r\n      <Goback />\r\n      {loading ? (\r\n        <Loader />\r\n      ) : error ? (\r\n        <Message\r\n          type='error'\r\n          msg={\r\n            error.includes(\"timed out\")\r\n              ? \"Network Error\"\r\n              : error.includes(\"mongo\")\r\n              ? \"Server Error\"\r\n              : error\r\n          }\r\n        />\r\n      ) : (\r\n        <div className='details'>\r\n          <div className='productImg'>\r\n            <img src={product.image} alt='product' />\r\n            <div className='bottomInfo'>\r\n              <h3>${product.price}</h3>\r\n\r\n              <div className='selectMobile'>\r\n                <h1>\r\n                  {product.countInStock !== 0 ? \"In Stock\" : \"Out Of Stock\"}\r\n                </h1>\r\n                {product.countInStock !== 0 && (\r\n                  <QtySelector\r\n                    cartCount={cartCount}\r\n                    setC